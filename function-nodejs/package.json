{
  "name": "reportingwithai-agent-nodejs",
  "version": "2.0.0",
  "description": "Node.js/TypeScript Lambda for chart generation with Claude Agent SDK",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "build:prod": "NODE_ENV=production tsup",
    "dev": "ts-node src/manual_run.ts",
    "dev:dist": "node dist/manual_run.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.700.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^18.19.59",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.39.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
