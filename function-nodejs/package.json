{
  "name": "reportingwithai-agent-nodejs",
  "version": "2.0.0",
  "description": "Node.js/TypeScript Lambda for chart generation with LangChain",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "build:prod": "NODE_ENV=production tsup",
    "dev": "tsx src/manual_run.ts",
    "dev:dist": "node dist/manual_run.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.700.0",
    "@langchain/anthropic": "^1.1.3",
    "@langchain/mcp-adapters": "^1.0.1",
    "dotenv": "^16.4.5",
    "langchain": "^1.1.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.39.1",
    "tsup": "^8.5.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
